files:
  "/etc/httpd/conf.d/custom_log.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      LogFormat "apache-access supplier-frontend \"%{DM-Request-ID}i\" %h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-Agent}i\"" cloudwatchlogs
      CustomLog logs/cwl_access_log cloudwatchlogs
